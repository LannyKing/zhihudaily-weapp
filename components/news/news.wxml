

<!--news.wxml
日报列表
-->
<template name="news_item">
    <view class="news_item" id="{{id}}" bindtap="dailyDetail">
        <view class="fl_left" style="width:200px">
        {{title}}
        </view>
        <view class="fl_right">
            <image style="width:78px;height:78px;" src="{{images}}"></image>
        </view>
        <view class="clearfix"></view>
    </view>

</template>


